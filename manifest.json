{
  "manifest_version": 3,
  "name": "Embiggen Google Docs Cursor",
  "description": "Makes your Google Docs cursor bigger",
  "version": "0.0.0.1",
  "content_scripts": [
    {
        "js": ["egdc.js"],
        "matches": [ "https://docs.google.com/document/*", "http://docs.google.com/document/*" ]
    }
  ] 
}
